    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "GEVENT_SUPPORT": "", 
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
